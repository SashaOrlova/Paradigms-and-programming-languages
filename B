#include <vector>
#include "optimization.h"
#include <iostream>
#include <cstdio>
using namespace std;
vector < int > a[100001];
int answer[100001];
int cnt( int x ){
        if ( answer[x] != 0) return answer[x];
        int j = a[x].size()-1;
        if (j == -1) return 2;
        while (j+1){
            answer[x] = max(answer[x], cnt(j) );
            a[x].pop_back();
            j--;
        }
        if (answer[x] == 2 ) answer[x] = 1; else answer[x] = 2;
return answer[x];
}
int main() {
    freopen("chess.in", "r", stdin);
    freopen("chess.out", "wt", stdout);
    int n = readInt();
    for (int i = 1; i <= n; i++){
        int p = readInt();
        a[p].push_back(i);
    }
    for (int i = 0; i <= n; i++){
        int j = a[i].size()-1;
        if (j == -1) continue;
        cout << a[i][j] << "\n";
    }
    if (cnt(0) == 2) writeWord("NO"); else writeWord("YES");
return 0;
}
